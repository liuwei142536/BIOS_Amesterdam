#include "VFR.uni"
#ifndef x-UEFI-AMI
#langdef   x-UEFI-AMI "x-UEFI-AMI"    // AMI Configuration Language
#endif


////////////////////Mapping language following: GSIO0xx////////////////////
////////////////////GSIO000 ------------------- GSIO099////////////////////
#string STR_SIO_CONFIGURATION           #language eng "SIO Configuration"
#string STR_SIO_CONFIGURATION_HELP      #language eng "SIO Chip configuration. Enable or Disable SIO Logical Devices, Resources and Features settings, etc."
