#string STR_TIME_6MIN                               #language en-US  "6 minutes"
#string STR_TIME_10MIN                              #language en-US  "10 minutes"
#string STR_TIME_15MIN                              #language en-US  "15 minutes"
#string STR_TIME_20MIN                              #language en-US  "20 minutes"



#string STR_SRVRMGMT_FRB_2ENABLE                    #language en-US  "FRB-2 Timer"
                                                    #language x-AMI  "IPMI100"
#string STR_SRVRMGMT_FRB_2ENABLE_HELP               #language en-US  "Enable or Disable FRB-2 timer(POST timer)"

#string STR_SRVRMGMT_FRB_TIMEOUT                    #language en-US  "FRB-2 Timer timeout"
                                                    #language x-AMI  "IPMI101"
#string STR_SRVRMGMT_FRB_TIMEOUT_HELP               #language en-US  "Enter value Between 3 to 6 min for FRB-2 Timer Expiration value"

#string STR_SRVRMGMT_FRB_TIMEOUT_POLICY             #language en-US  "FRB-2 Timer Policy"
                                                    #language x-AMI  "IPMI102"
