#string STR_RESET                                   #language en-US  "Reset"
#string STR_POWERDOWN                               #language en-US  "Power Down"
#string STR_DO_NOTHING                              #language en-US  "Do Nothing"
#string STR_POWERCYCLE                              #language en-US  "Power Cycle"
#string STR_TIME_3MIN                               #language en-US  "3 minutes"
#string STR_TIME_4MIN                               #language en-US  "4 minutes"
